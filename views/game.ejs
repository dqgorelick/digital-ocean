<!DOCTYPE html>
<html>
<head>
    <title>Digital Ocean!</title>
    <link rel="stylesheet" href="/dist/index.css">
      <script src="/bower_components/jquery/dist/jquery.min.js"></script>
      <script src="/public/js/vendor/JareUtils.js"></script>
      <script src="/public/js/vendor/GameLoopManager.js"></script>
      <script src="/public/js/game_core.js"></script>
      <script src="/public/js/player.js"></script>
      <script src="https://cdn.socket.io/socket.io-1.4.3.js"></script>
</head>
<body>
  <section class="game">
    <div class="wrap">
      <h1>Sharks and Minnows</h1>
    </div>
    <canvas id="canvas" width="750" height="450"></canvas>
    <div class="status">
      <span>No connection to server :(</span>
    </div>
    <div class="info">
      <span id="username"><%=username%></span> <span id="controls"><%=controls%></span>
    </div>
  </section>
  <audio autoplay="" loop="" preload="">
    <source src="/public/audio/Digital Ocean Main Theme.mp3" type="audio/mpeg">
  </audio>
  <script src="/public/js/index.js"></script>
</body>
</html>
